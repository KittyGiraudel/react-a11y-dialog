{
  "compilerOptions": {
		"baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "declaration": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "moduleResolution": "node",
    "outDir": "dist",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
	"include": ["src/**/*"]
}
